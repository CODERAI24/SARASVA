{
  "firestore": {
    "rules": "firestore.rules"
  },
  "hosting": {
    "public": "firebase-public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      { "source": "/", "destination": "/SARASVA/", "type": 301 }
    ],
    "rewrites": [
      {
        "source": "/SARASVA/**",
        "destination": "/SARASVA/index.html"
      }
    ]
  }
}
